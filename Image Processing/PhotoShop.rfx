     <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_2_0" ENTRY_POINT="ps_main" COMPILER_FLAGS="0"><RmShaderCode TYPE="TEXT"><![CDATA[
sampler2D Texture0;
float2 TexSize;
bool useSimple;
bool useSobel_h;
bool useSobel_v;

float3 Filter3x3(float3x3 filter, sampler2D image, float2 xy, float2 texSize)
{
    float2 filterPosOffset[3][3] =
    {
        {float2(-1, -1), float2(0, -1), float2(1, -1)},
        {float2(-1,  0), float2(0,  0), float2(1,  0)},
        {float2(-1,  1), float2(0,  1), float2(1,  1)}
    };

    float3 finalColor = 0;

    for (int i = 0; i < 3; i++)
    {
        for (int j = 0; j < 3; j++)
        {
            float2 newXY = (xy + filterPosOffset[i][j]) / texSize;
            finalColor.rgb += tex2D(image, newXY).rgb * filter[i][j];
        }
    }

    return finalColor;
}

float4 ps_main( float2 texCoord  : TEXCOORD0 ) : COLOR
{
    float2 intXY = int2(texCoord * TexSize);
    float3x3 SimpleFilter = float3x3(-0.5, -1, 0, 1, 0, 1, -0, 1, 0.5);
    float3x3 SobelFilter_h = float3x3(-1, -2, -1, 0, 0, 0, 1, 2, 1);
    float3x3 SobelFilter_v = transpose(SobelFilter_h);

    float3x3 filter;
    if (useSimple)
        filter = SimpleFilter;
