     <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_2_0" ENTRY_POINT="ps_main" COMPILER_FLAGS="0"><RmShaderCode TYPE="TEXT"><![CDATA[
sampler2D Texture0;
sampler2D TexDownSample;
float k;
bool useHDR;

float4 xposure(float4 color, float gray, float ex)
{
    float b = ex * 4 - 1;
    float a = 1 - b;
    float f = gray * (a * gray + b);
    return f * color;
}

float4 ps_main( float2 texCoord  : TEXCOORD0 ) : COLOR
{
    float4 dsColor = tex2D(TexDownSample, texCoord);
    float lum = dot( float3(0.3, 0.59, 0.11), dsColor.rgb);
    float4 fColor = tex2D(Texture0, texCoord);

    if (useHDR)
        return xposure(fColor, lum, k);
    else
        return tex2D( Texture0, texCoord );
}

]]></RmShaderCode>
