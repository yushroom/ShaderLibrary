     <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_2_0" ENTRY_POINT="ps_main" COMPILER_FLAGS="0"><RmShaderCode TYPE="TEXT"><![CDATA[
sampler2D Texture0;
float2 TexSize;
float2 mosaicSize = {32, 32};

float4 ps_main( float2 texCoord  : TEXCOORD0 ) : COLOR
{
    float2 intXY = texCoord * TexSize;
    float2 XYMosaic = int2(intXY / mosaicSize) * mosaicSize + 0.5 * mosaicSize;
    float2 delXY = XYMosaic - intXY;
    float delL = length(delXY);
    float2 UVMosaic = XYMosaic / TexSize;
    bool inCycle = delL < 0.5 * mosaicSize.x;
    return tex2D(Texture0, UVMosaic) * inCycle + tex2D(Texture0, texCoord) * (1-inCycle);
}

]]></RmShaderCode>
